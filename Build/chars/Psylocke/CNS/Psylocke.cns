;Variables
;var(1)   = Unique Comboing Decider and wait time
;var(2)   = Hitadd for Strong Jump Kick Version 2 Hitdef
;var(3)   = Velocity set for Super Jump
;var(4)   = Random X for PsyStars
;var(5)   = Random Y for PsyStars
;var(6)   = PsyBlade Enable/Disable
;var(7)   = Change Jumpland for PsyBlade
;var(8)   = Mini Psy-Thrust Controller
;var(9)   = PsyThrust Continue Decider
;var(10)  = Standing Strong Kick sound decider
;var(11)  = Super Jump
;var(12)  = X Position for Ninjitsu B
;var(13)  = Y Position for Ninjitsu B
;var(14)  = Psythrust Navigation
;var(15)  = Kochou Gakure Snd Trigger
;var(16)  = Super Finish Words Display: 1:Psi-Thrust  2:Maelstrom  3:Kochou
;var(17)  = Stop Super Finish if looping
;var(18)  = Combo Hit Count
;var(19)  = Extra Stuff for Super Finish
;var(20)  = Recovery Roll
;var(21)  = Introduction/Winstate Random Decider
;var(22)  = Introduction #2 Ninjitsu Teleport Random Placement
;var(57)  = AI-Activation

[Data]
life = 1000			;Amount of life to start with
attack = 100		;attack power (more is stronger)
defence = 100		;defensive power (more is stronger)
power = 5000
fall.defence_up = 50	;Percentage to increase defense everytime player is knocked down
liedown.time = 15		;Time which player lies down for, before getting up
airjuggle = 50		;Number of points for juggling
sparkno = 2			;Default hit spark number for HitDefs
guard.sparkno = 40	;Default guard spark number
KO.echo = 0			;1 to enable echo on KO
volumescale = 155
volume = 155
IntPersistIndex = 60	;Variables with this index and above will not have their values
FloatPersistIndex = 40  ;reset to 0 between rounds or matches. There are 60 int variables,
  				;indexed from 0 to 59, and 40 float variables, indexed from 0 to 39.
  				;If omitted, then it defaults to 60 and 40 for integer and float
  				;variables repectively, meaning that none are persistent, i.e. all
  				;are reset. If you want your variables to persist between matches,
  				;you need to override state 5900 from common1.cns.


[Size]
xscale = .83
yscale = 1.07  
ground.back = 22
ground.front = 18
air.back = 23		;Player width (back, air)
air.front = 24		;Player width (front, air)
height = 60			;Height of player (for opponent to jump over)
attack.dist = 160		;Default attack distance
proj.attack.dist = 90	;Default attack distance for projectiles
proj.doscale = 0		;Set to 1 to scale projectiles too   
head.pos = -5, -90	;Approximate position of head
mid.pos = -5, -60		;Approximate position of midsection
shadowoffset = 0		;Number of pixels to vertically offset the shadow
draw.offset = 0,-3		;Player drawing offset in pixels (x, y)

[Velocity]
walk.fwd  = 3.8		;Walk forward
walk.back = -3		;Walk backward
run.fwd  = 4.6, 0		;Run forward (x, y)
run.back = -4.5,-3.8	;Hop backward (x, y)
jump.neu = 0,-12	;Neutral jumping velocity (x, y)
jump.back = -3.6,-3		;Jump back Speed (x, y)
jump.fwd = 4.2,-3		;Jump forward Speed (x, y)
runjump.back = -2.55,-3
runjump.fwd = 5.5,-3
airjump.neu = 0,-12	;.
airjump.back =  -3.3	;Air jump speeds (opt)
airjump.fwd = 3.6		;.

[Movement]
airjump.num = 2		;Number of air jumps allowed (opt)
airjump.height = 35	;Minimum distance from ground before you can air jump (opt)
yaccel = .68
stand.friction = .8
crouch.friction = .8


