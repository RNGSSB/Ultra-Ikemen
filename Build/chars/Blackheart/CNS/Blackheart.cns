physics = s
;anim = 40
ctrl = 0
sprpriority = 1

[state 41, changestate]
type = varset
trigger1 = time = 0
v = 8
value = 0

[state 40, varset]
type = varset
trigger1 = !time
sysvar(1) = 0

[state 40, varset]
type = varset
trigger1 = command = "holdfwd"
sysvar(1) = 1

[state 40, varset]
type = varset
trigger1 = command = "holdback"
sysvar(1) = -1

[state 40, velset]
type = velset
trigger1 = !var(45)
trigger1 = !animtime
x = ifelse(!sysvar(1), const(velocity.jump.neu.x), ifelse((sysvar(1) = 1), const(velocity.jump.fwd.x), const(velocity.jump.back.x)))
