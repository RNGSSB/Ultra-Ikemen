[state 40, velset]
type = velset
trigger1 = var(45)
trigger1 = !animtime
x = ifelse(!sysvar(1), const(velocity.jump.neu.x), ifelse((sysvar(1) = 1), const(velocity.jump.fwd.x), const(velocity.jump.back.x))) * 1.5
y = const(velocity.jump.y)

[state 40, velset]
type = velset
trigger1 = !var(45)
trigger1 = !animtime
trigger1 = prevstateno = 100
trigger1 = sysvar(1) = 1
x = const(velocity.runjump.fwd.x)

[state 40, velset]
type = velset
trigger1 = var(45)
trigger1 = !animtime
trigger1 = prevstateno= 100
trigger1 = sysvar(1) = 1
x = const(velocity.runjump.fwd.x) * 1.5

[state 40, changeanim]
type = changeanim
trigger1 = !time && anim != 40
value = 40

[state 40, changestate]
type = changestate
trigger1 = !animtime
value = 50
ctrl = 1

