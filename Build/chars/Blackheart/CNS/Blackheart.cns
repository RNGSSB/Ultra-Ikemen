trigger1 = vel x > -1
trigger2 = sysvar(1) = 1
trigger2 = vel x > 2.5
x = -.15

[state 50, changeanim]
type = changeanim
trigger1 = anim != 41
trigger1 = anim != 42
trigger1 = anim != 43
trigger1 = vel y < 0
trigger1 = !time
trigger2 = var(10)
trigger2 = animelem = 5
trigger2 = vel y < -8.5
value = (ifelse((vel x)=0, 41, ifelse((vel x)>0, 42, 43)))

[state 50, changeanim]
type = changeanim
trigger1 = anim = 41 || anim = 42 || anim = 43 || anim = 10041 || anim = 10042 || anim = 10043
trigger1 = !animtime
trigger2 = anim != 41 && anim != 42 && anim != 43 && anim != 44 && anim != 10041 && anim != 10042 && anim != 10043
trigger2 = vel y > 0
value = 44

[state 50, turn]
type = turn
trigger1 = var(8) = 1
trigger1 = p2dist x < 0

