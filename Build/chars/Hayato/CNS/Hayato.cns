; var(10) - 汎用
; var(11) - アドバンシングガード
; var(12) - 特殊ゲージ時間
; var(13) - 空中コンボフラグ
; var(14) - システムメッセージ
; var(15) - 空中浮遊
; var(16) - ゲージ位置補正
; var(17) - 連打入力
; var(18) - コンボ数
; var(19) - 空中でのコンボ数
; var(20) - 画面外矢印表示用(自分用)
; var(21) - 画面外矢印表示用(相手用)

; var(40) - システムメッセージ表示
; var(41) - システムメッセージY座標
; var(58) -
; var(59) - AI

; fvar(0) - 攻撃力補正
; fvar(1) - 重力補正
; fvar(2) - 汎用
; fvar(3) - TIMEゲージX座標
; fvar(4) - TIMEゲージY座標
; fvar(5) -
; fvar(6) -
; fvar(7) -
; fvar(8) -
; fvar(9) -
 
