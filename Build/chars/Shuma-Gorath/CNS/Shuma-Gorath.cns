[Velocity]
walk.fwd  = 4     ;Walk forward
walk.back = -2.9    ;Walk backward
run.fwd  = 6, 0   ;Run forward (x, y)
run.back = -4.125 ;Hop backward (x, y)
jump.neu = 0,-12   ;Neutral jumping velocity (x, y)
jump.back = -3.4,-12;Jump back Speed (x, y)
jump.fwd = 3.4,-11.6     ;Jump forward Speed (x, y)
runjump.back = -3,-8.9375 ;Running jump speeds (opt)
runjump.fwd = 5,-8.9375  ;.
airjump.neu = 0,-32.4     ;.
airjump.back = -10.2      ;Air jump speeds (opt)
airjump.fwd = 10          ;.
air.gethit.groundrecover = -.6,-2    ;Velocity for ground recovery state (x, y) **MUGEN 1.0**
air.gethit.airrecover.mul = .5,.2     ;Multiplier for air recovery velocity (x, y) **MUGEN 1.0**
air.gethit.airrecover.add = 0,-0.1    ;Velocity offset for air recovery (x, y) **MUGEN 1.0**
air.gethit.airrecover.back = -2       ;Extra x-velocity for holding back during air recovery **MUGEN 1.0**
air.gethit.airrecover.fwd = 0         ;Extra x-velocity for holding forward during air recovery **MUGEN 1.0**
air.gethit.airrecover.up = -3         ;Extra y-velocity for holding up during air recovery **MUGEN 1.0**
air.gethit.airrecover.down = 1      ;Extra y-velocity for holding down during air recovery **MUGEN 1.0**

[Movement]
airjump.num = 0      ;Number of air jumps allowed (opt)
airjump.height = 35 ;Minimum distance from ground before you can air jump (opt)
yaccel = 0.634; 0.3359375       ;Vertical acceleration
stand.friction = 0.8     ;Friction coefficient when standing
crouch.friction = .8     ;Friction coefficient when crouching
stand.friction.threshold = 8          ;If player's speed drops below this threshold while standing, stop his movement **MUGEN 1.0**
crouch.friction.threshold = .2        ;If player's speed drops below this threshold while crouching, stop his movement **MUGEN 1.0**
air.gethit.groundlevel = 10          ;Y-position at which a falling player is considered to hit the ground **MUGEN 1.0**
air.gethit.groundrecover.ground.threshold = -20 ;Y-position below which falling player can use the recovery command **MUGEN 1.0**
air.gethit.groundrecover.groundlevel = 0   ;Y-position at which player in the ground recovery state touches the ground **MUGEN 1.0**
air.gethit.airrecover.threshold = -4  ;Y-velocity above which player may use the air recovery command **MUGEN 1.0**
air.gethit.airrecover.yaccel = 0.55    ;Vertical acceleration for player in the air recovery state **MUGEN 1.0**
air.gethit.trip.groundlevel = 0     ;Y-position at which player in the tripped state touches the ground **MUGEN 1.0**
down.bounce.offset = 0, 0            ;Offset for player bouncing off the ground (x, y) **MUGEN 1.0**
down.bounce.yaccel = 0.55              ;Vertical acceleration for player bouncing off the ground **MUGEN 1.0**
down.bounce.groundlevel = 0       ;Y-position at which player bouncing off the ground touches the ground again **MUGEN 1.0**
down.friction.threshold = .2          ;If the player's speed drops below this threshold while lying down, stop his movement **MUGEN 1.0**

;---------------------------------------------------------------------------
; Variable usage:

;var(1): Air Dash
;var(2): Jump type. 1 = canceled, 0/2 = off. 3 = jumping forward on cancel, 4 = back on cancel
;var(3): Basic Throw direction, back/forward.
;var(5): Waste variable for hit spark positioning, use as needed.
;var(6): Mystic Stare has been launched. Can't do another one
;var(8): throw is ground hit. 0 if allowed, 1 if not. Prevstateno = reset
;var(11): Power gain Dampener
;var(13): Extra var needed because of mistake and this is an easy solution.
;var(14): Combo Counter, reset to 0 by helper, if it's over 100, play air combo
;var(20): Chaos Dimension Timer

