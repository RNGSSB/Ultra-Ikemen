trigger2 = command = "x" || command = "y" || command = "z"
source = 1,1
dest = 1,1+random%23


;---------------------------------------------------------------------------
; States that are executed when in self's state file (use statedef -3)
;---------------------------------------------------------------------------

[Statedef -3]

[State VelSet]
type = VelSet
triggerall = stateno = 101 && animelemtime(2) > 2
trigger1 = command = "a" || command = "b" || command = "c"
trigger2 = command = "x" || command = "y" || command = "z"
x = ifelse(animelemtime(2) <= 2,0,5)

[State PosSet]
type = PosSet
trigger1 = prevstateno = 105 && stateno = [200,260]
y = 0

[state -3]
type = targetstate
triggerall = !winko
triggerall = numtarget(100) = 1
triggerall = target(100), hitover
trigger1 = stateno != 811 ;|| (stateno = 811 && time = 77)
trigger1 = stateno != 821
trigger1 = stateno != 825 || (stateno = 825 && time = 30)
trigger1 = p2stateno != [120,155]
value = 9040

;Reset Air Dash control
[state -3]
type = varset
trigger1 = statetype = S && var(1) != 0
trigger2 = stateno >= 1000 && var(1) != 0
v = 1
value = 0

[state -3, dampener]
type = attackmulset
trigger1 = numtarget >= 1
trigger1 = stateno != [3000,3012]
value = 0.90 ** target, gethitvar(hitcount)

[State -3, VarSet]
type = VarSet
triggerall = numtarget
trigger1 = movehit = 1
trigger2 = numprojID(500) = 1
v = 14
value = var(14)+1

[state -3, dampener]
type = attackmulset
triggerall = numenemy
trigger1 = enemynear(0), stateno = 5040
trigger2 = enemynear(0), stateno = 5120
value = 1

[state -3, power dampener]
type = varset
trigger1 = numtarget = 0
trigger1 = var(11) > 0
v = 11
value = 1

[state -3, power dampener]
type = varset
trigger1 = numtarget >= 1
v = 11
value = target,gethitvar(hitcount)

; _[W{CX 
[State 1120, _[W{CX]
type = PlaySnd
triggerall = alive && movetype = H
triggerall = random % 10 < 5
trigger1 = stateno = 5000 && time = 1
trigger2 = stateno = 5010 && time = 1
trigger3 = stateno = 5020 && time = 1
trigger4 = stateno = 5070 && time = 1
trigger5 = stateno = 5080 && time = 1
value = s5000,random%2
channel = 2
volume = 200
volumescale = 200

[State 0, StopSnd]
type = StopSnd
trigger1 =  !alive
channel =2
