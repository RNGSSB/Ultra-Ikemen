; Variable usage: 
; var(0)  - モード
; var(1)  - スーパージャンプフラグ
; var(2)  - 空中ジャンプ回数
; var(3)  - 上押しっぱなし判定
; var(4)  - ヘルパー通信用
; var(5)  - 画面外フラグ
; var(6)  - リミッターフラグ
; var(7)  - ターゲットID
; var(8)  - 叩きつけフラグ
; var(9)  - ダウン追撃フラグ
; var(10) - 汎用
; var(11) - アドバンシングガード
; var(12) - 特殊ゲージ時間
; var(13) - 空中コンボフラグ
; var(14) - システムメッセージ
; var(15) - 空中浮遊
; var(16) - ゲージ位置補正
; var(17) - 連打入力
; var(18) - コンボ数
; var(19) - 空中でのコンボ数
; var(20) - 画面外矢印表示用

; var(30) - シュバルツェール回数
; var(31) - シュバルツェールヒット回数

; var(40) - システムメッセージ表示
; var(41) - システムメッセージY座標
; var(58) -
; var(59) - AI

; fvar(0) - 攻撃力補正
; fvar(1) - 重力補正
; fvar(2) - 汎用
; fvar(3) - TIMEゲージX座標
; fvar(4) - TIMEゲージY座標
; fvar(5) -
; fvar(6) -
; fvar(7) -
; fvar(8) -
; fvar(9) -
 
